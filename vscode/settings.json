{
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.autoSwitchInputMethod.enable": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "vspacecode.space"
      ]
    },
    {
      "before": [
        ","
      ],
      "commands": [
        "vspacecode.space",
        {
          "command": "whichkey.triggerKey",
          "args": "m"
        }
      ]
    },
    {
      "before": [
        "g",
        "h"
      ],
      "command": [
        "editor.action.showHover"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "vspacecode.space"
      ]
    },
    {
      "before": [
        ","
      ],
      "commands": [
        "vspacecode.space",
        {
          "command": "whichkey.triggerKey",
          "args": "m"
        }
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  "editor.renderWhitespace": "all",
  "editor.inlineSuggest.enabled": true,
  "editor.fontSize": 15,
  "editor.fontLigatures": false,
  "editor.formatOnSave": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.cursorSurroundingLines": 1000,
  "editor.scrollBeyondLastLine": false,
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "editor.renderLineHighlight": "all",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "Default Dark+",
  "workbench.editor.showTabs": "single",
  "workbench.editor.centeredLayoutAutoResize": false,
  "workbench.editor.centeredLayoutFixedWidth": true,
  "workbench.settings.applyToAllProfiles": [
    "vim.autoSwitchInputMethod.enable"
  ],
  "window.commandCenter": true,
  "window.newWindowProfile": "Default",
  "search.actionsPosition": "auto",
  "search.mode": "newEditor",
  "search.collapseResults": "alwaysCollapse",
  "search.showLineNumbers": true,
  "search.searchEditor.defaultNumberOfContextLines": 0,
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "files.watcherExclude": {
    "**/.git/**": true
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "vspacecode.bindingOverrides": [
    {
      "keys": "n",
      "name": "+narrow/focus",
      "type": "bindings",
      "bindings": [
        {
          "key": "w",
          "name": "Widen Region",
          "icon": "debug-rerun",
          "type": "command",
          "command": "extension.focus"
        },
        {
          "key": "r",
          "name": "Narrow Region",
          "icon": "debug-rerun",
          "type": "command",
          "command": "extension.focus"
        }
      ]
    },
    {
      "keys": "c.r",
      "name": "Rerun last task",
      "icon": "debug-rerun",
      "type": "command",
      "command": "workbench.action.tasks.reRunTask"
    },
    {
      "keys": "c.d",
      "name": "Close compile panel",
      "icon": "output",
      "type": "command",
      "command": "workbench.action.togglePanel"
    },
    {
      "keys": "f.j",
      "name": "Open file/folder",
      "icon": "folder-opened",
      "type": "command",
      "command": "file-browser.open"
    },
    {
      "keys": "'",
      "name": "Show terminal",
      "icon": "terminal",
      "type": "command",
      "command": "workbench.action.terminal.newWithCwd",
      "args": {
        "cwd": "\"${fileDirname}\""
      }
    },
    {
      "keys": "p.'",
      "name": "Show terminal",
      "icon": "terminal",
      "type": "command",
      "command": "workbench.action.terminal.focus"
    },
    {
      "keys": "m.languageId:cpp.g.a",
      "name": "Switch Header/Source",
      "type": "command",
      "icon": "files",
      "command": "clangd.switchheadersource"
    },
    {
      "keys": "m.languageId:cpp.h",
      "name": "Describe thing at point",
      "icon": "book",
      "type": "command",
      "command": "editor.action.showHover"
    },
    {
      "keys": "f.j",
      "name": "Dired Open File/Folder",
      "type": "command",
      "icon": "file-directory",
      "command": "extension.dired.open"
    },
    {
      "keys": "r.y",
      "name": "Clipboard Yank",
      "icon": "copy",
      "type": "command",
      "command": "clipboard-manager.editor.pickAndPaste"
    }
  ],
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.autoSave": "afterDelay",
  "[feature]": {
    "editor.formatOnSave": false
  },
  "git.blame.editorDecoration.enabled": true,
  "[magit]": {
    "editor.renderWhitespace": "all"
  },
  "dired.ask_directory": false
}
